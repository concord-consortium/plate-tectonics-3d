{
  "name": "tectonic-explorer",
  "version": "1.3.0",
  "main": "js/components/app.js",
  "scripts": {
    "lint": "standard --fix",
    "lint:test": "standard",
    "test": "jest",
    "test:watch": "jest --watch --watchPathIgnorePatterns model-serialization.test.js",
    "start": "webpack serve",
    "build": "webpack",
    "build-production": "PRODUCTION=true webpack",
    "test:cypress": "cypress run --reporter cypress-image-snapshot/reporter",
    "test:cypress:basic": "cypress run --spec cypress/integration/basic/**/*",
    "test:cypress:snapshots": "cypress run --spec cypress/integration/gpu-dependant/**/* --env failOnSnapshotDiff=false --reporter cypress-image-snapshot/reporter",
    "test:cypress:open": "cypress open --env failOnSnapshotDiff=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/tectonic-explorer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/tectonic-explorer/issues"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test"
    ],
    "globals": [
      "jest",
      "describe",
      "context",
      "test",
      "it",
      "specify",
      "expect",
      "before",
      "beforeAll",
      "beforeEach",
      "afterEach",
      "cy",
      "Cypress"
    ]
  },
  "jest": {
    "testURL": "https://tectonic-explorer.jest-test.com",
    "setupFilesAfterEnv": [
      "<rootDir>test/setup-tests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/concord-consortium/tectonic-explorer#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "d3-color": "^2.0.0",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.3",
    "eventemitter2": "^6.4.3",
    "firebase": "^8.2.7",
    "jquery": "^3.5.1",
    "kd-tree-javascript": "^1.0.3",
    "lodash": "^4.17.20",
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "point-in-polygon": "^1.0.1",
    "postcss": "^8.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-toolbox": "2.0.0-beta.13",
    "react-transition-group": "~4.4.1",
    "rollbar": "^2.20.0",
    "screenfull": "^5.1.0",
    "seedrandom": "^3.0.5",
    "shutterbug": "^1.3.2",
    "three": "^0.125.2",
    "timeseries-analysis": "^1.0.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@svgr/webpack": "^5.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "cypress": "^3.3.1",
    "cypress-image-snapshot": "^4.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "^6.2.0",
    "get-pixels": "^3.3.2",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^2.0.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^5.0.0",
    "raw-loader": "^4.0.2",
    "react-sortable-hoc": "^1.11.0",
    "standard": "^12.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.1",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
