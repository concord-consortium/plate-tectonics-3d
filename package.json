{
  "name": "tectonic-explorer",
  "version": "1.1.0",
  "main": "js/components/app.js",
  "scripts": {
    "lint": "standard --fix",
    "lint:test": "standard",
    "test": "jest",
    "start": "webpack-dev-server",
    "build": "webpack",
    "build-production": "PRODUCTION=true webpack",
    "test:cypress": "cypress run --reporter cypress-image-snapshot/reporter",
    "test:cypress:basic": "cypress run --spec cypress/integration/basic/**/*",
    "test:cypress:snapshots": "cypress run --spec cypress/integration/gpu-dependant/snapshots.js --env failOnSnapshotDiff=false --reporter cypress-image-snapshot/reporter",
    "test:cypress:open": "cypress open --env failOnSnapshotDiff=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/tectonic-explorer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/tectonic-explorer/issues"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "test"
    ],
    "globals": [
      "jest",
      "describe",
      "context",
      "test",
      "it",
      "specify",
      "expect",
      "before",
      "beforeAll",
      "beforeEach",
      "afterEach",
      "cy",
      "Cypress"
    ]
  },
  "jest": {
    "testURL": "https://tectonic-explorer.jest-test.com",
    "setupTestFrameworkScriptFile": "<rootDir>test/setup-tests.js",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/concord-consortium/tectonic-explorer#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "d3-color": "^1.2.3",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^1.0.7",
    "eventemitter2": "^4.1.0",
    "firebase": "^5.8.1",
    "jquery": "^3.3.1",
    "kd-tree-javascript": "^1.0.1",
    "lodash": "^4.17.11",
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "point-in-polygon": "^1.0.1",
    "postcss": "^6.0.23",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-toolbox": "2.0.0-beta.12",
    "react-transition-group": "1.x",
    "rollbar-browser": "^1.9.4",
    "screenfull": "^4.0.0",
    "seedrandom": "^2.4.4",
    "shutterbug": "^1.1.0",
    "three": "^0.101.1",
    "timeseries-analysis": "^1.0.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.5",
    "cypress-image-snapshot": "^3.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "file-loader": "^0.9.0",
    "get-pixels": "^3.3.2",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^24.1.0",
    "json-loader": "^0.5.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.6",
    "raw-loader": "^0.5.1",
    "react-sortable-hoc": "^0.6.8",
    "react-test-renderer": "^15.5.4",
    "standard": "^12.0.1",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
